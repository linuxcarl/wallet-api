{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier",
    "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single"],
    "no-console": false,
    "trailing-comma": false,
    "import-name": [false],
    "max-line-length": [
      true,
      {
        "limit": 120,
        "ignore-pattern": "^import [^,]+ from |^export | implements"
      }
    ],
    "variable-name": [true, "allow-snake-case"]
  },
  "rulesDirectory": []
}
